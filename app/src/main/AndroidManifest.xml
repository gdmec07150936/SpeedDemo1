<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.speeddemo">
    <!-- if you want to load images from the internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- if you want to load images from a file OR from the internet -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".ClientApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity android:name=".mAdmin.Choose_Date">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mAdmin.Dindan_Activity"></activity>
        <activity android:name=".mAdmin.LoginActivity"></activity>
        <activity android:name=".mAdmin.MyInfoActivity"></activity>
        <activity android:name=".mAdmin.beginActivity"></activity>
        <activity android:name=".mAdmin.homeActivity"></activity>
        <activity android:name=".mAdmin.Price_Inquiry_Activity"></activity>
        <activity android:name=".mAdmin.WebViewActivity"></activity>

    </application>

</manifest>